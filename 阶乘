#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main(void)
{
	int i, j;
	unsigned long long k = 1;
loop:
	printf("请输入一个整数:");
	scanf("%d", &i);
	if (i < 0)
	{
		printf("这是一个负数，没有阶乘。\n");
		goto loop;
	}
	else
	{
		for (j = 1; j <= i; j++)
		{
			k = k * j;
		}
	}
	printf("%d!=%llu", i, k);
	return 0;
}
